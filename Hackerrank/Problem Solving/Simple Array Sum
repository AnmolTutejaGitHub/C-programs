#include<stdio.h>

int arraySum(int arr[],int n,int sum)
{
    for(int i=0;i<n;i++)
    sum = sum + arr[i];
    
    return sum;
}

int main()
{   
    int n,sum=0,i;
    int a[1000];
    scanf("%d",&n);
    
    for(i=0;i<n;i++)
    scanf("%d",&a[i]);
  
    printf("%d",arraySum(a,n,sum));
    
return 0;
}
